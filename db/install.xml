<?xml version="1.0" encoding="UTF-8" ?>
<XMLDB PATH="question/type/cloud/db" VERSION="20130204" COMMENT="XMLDB file for Moodle question/type/cloud"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="../../../../lib/xmldb/xmldb.xsd"
>
  <TABLES>
    <TABLE NAME="question_cloud_account" COMMENT="Account settings for cloud type." NEXT="question_cloud_server">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="true" NEXT="question"/>
        <FIELD NAME="question" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="false" PREVIOUS="id" NEXT="username"/>
        <FIELD NAME="username" TYPE="char" LENGTH="128" NOTNULL="true" SEQUENCE="false" PREVIOUS="question" NEXT="password"/>
        <FIELD NAME="password" TYPE="char" LENGTH="128" NOTNULL="true" SEQUENCE="false" PREVIOUS="username" NEXT="auth_token"/>
        <FIELD NAME="auth_token" TYPE="char" LENGTH="128" NOTNULL="false" SEQUENCE="false" PREVIOUS="password" NEXT="api_key"/>
        <FIELD NAME="api_key" TYPE="char" LENGTH="128" NOTNULL="true" SEQUENCE="false" PREVIOUS="auth_token" NEXT="api_auth_token"/>
        <FIELD NAME="api_auth_token" TYPE="char" LENGTH="128" NOTNULL="false" SEQUENCE="false" PREVIOUS="api_key"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
      </KEYS>
    </TABLE>
    <TABLE NAME="question_cloud_server" COMMENT="Contains cloud server setup data" PREVIOUS="question_cloud_account">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="true" NEXT="question"/>
        <FIELD NAME="question" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="false" PREVIOUS="id"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
      </KEYS>
    </TABLE>
  </TABLES>
</XMLDB>